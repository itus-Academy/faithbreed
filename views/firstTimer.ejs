        <script>
        
        var button1 = document.getElementById('button1');
        var space1 = document.getElementById('space1');
        var linksminus = document.getElementById('linkminus')
        var linksplus = document.getElementById('linksplus');
        but(button1,space1,linkplus1,linkminus1);
    
        var button2 = document.getElementById('button2');
        var space2 = document.getElementById('space2');
        var linksminus2 = document.getElementById('linkminus2')
        var linksplus2 = document.getElementById('linksplus2');
        but(button2,space2,linkplus2,linkminus2);
    
        var button3 = document.getElementById('button3');
        var space3 = document.getElementById('space3');
        var linksminus3 = document.getElementById('linkminus3')
        var linksplus3 = document.getElementById('linksplus3');
        but(button3,space3,linkplus3,linkminus3);
    
        var button4 = document.getElementById('button4');
        var space4 = document.getElementById('space4');
        var linksminus4 = document.getElementById('linkminus4')
        var linksplus4 = document.getElementById('linksplus4');
        but(button4,space4,linkplus4,linkminus4);
    
        function but (parbut, parspace,linkminus,linkplus){
            parbut.addEventListener('click', ()=>{
                if(parspace.style.display == 'block'){
                    parspace.style.display='none';
                    parspace.style.WebkitTransition = "all 2s"
                    linkminus.style.display='block';
                    linkplus.style.display = 'none';
                }else{
                    parspace.style.display='block';
                    linkminus.style.display='none';
                    linkplus.style.display = 'block';
                   
                }
            
                // alert('hello');
            })
        }
        
